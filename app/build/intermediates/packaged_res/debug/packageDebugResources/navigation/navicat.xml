<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navicat"
    app:startDestination="@id/welc_main">

    <fragment
        android:id="@+id/welc_main"
        android:name="com.example.obd_kotlin.welc_main"
        android:label="fragment_welc_main"
        tools:layout="@layout/fragment_welc_main" >
        <action
            android:id="@+id/action_welc_main_to_welc_auth"
            app:destination="@id/welc_auth"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/welc_auth"
        android:name="com.example.obd_kotlin.welc_auth"
        android:label="fragment_welc_auth"
        tools:layout="@layout/fragment_welc_auth" >
        <action
            android:id="@+id/action_welc_auth_to_welc_main"
            app:destination="@id/welc_main"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out" />
        <action
            android:id="@+id/action_welc_auth_to_welc_reg"
            app:destination="@id/welc_reg"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out" />
    </fragment>
    <fragment
        android:id="@+id/welc_reg"
        android:name="com.example.obd_kotlin.welc_reg"
        android:label="fragment_welc_reg"
        tools:layout="@layout/fragment_welc_reg" >
        <action
            android:id="@+id/action_welc_reg_to_welc_auth"
            app:destination="@id/welc_auth"
            app:enterAnim="@android:anim/fade_in"
            app:exitAnim="@android:anim/fade_out" />
    </fragment>
</navigation>